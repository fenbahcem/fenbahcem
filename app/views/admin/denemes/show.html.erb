<% end %>


<table class="table table-striped">
	<tr>
		<td><strong>Sınıf:</strong></td>
		<td><%= @admin_deneme.sinif %></td>
	</tr>
	<tr>
		<td><strong>Ünite Adı:</strong></td>
		<td><%= @admin_deneme.uniteadi %></td>
	</tr>
	<tr>
		<td><strong>Deneme Adı:</strong></td>
