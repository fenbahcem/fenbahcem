	</tr>
	<tr>
		<td><strong>Açıklama:</strong></td>
		<td><%= @admin_deneme.aciklama.html_safe %></td>
	</tr>
	<tr>
		<td><strong>Dosya</strong></td>
		<td><%= link_to 'Görüntüle', @admin_deneme.dosya.url %></td>
